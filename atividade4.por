
programa {
  funcao inicio() {
    inteiro temperatura
    escreva("Qual é a temperatura de hoje?\n")
    leia(temperatura)

    se (temperatura > 0) {
      escreva("A temperatura está positiva\n")
      
    } senao se (temperatura < 0) { 
      escreva("A temperatura está negativa\n")
      
    } 
    }
  }
